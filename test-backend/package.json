{
  "name": "energy-backend",
  "author": "Teknodelta Ltd. Sti.",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "build": "run-s clean && mkdir -p dist && babel src -D -d dist --ignore='src/**/*spec.js'",
    "prestart": "npm run -s build",
    "start": "node dist",
    "start-prod": "NODE_ENV=production run-s start",
    "dev": "nodemon -w src src/index --exec \"node -r babel-register\"",
    "test:run": "mocha test --recursive -r babel-register -r ./test/init --exit --timeout=10000",
    "test:modules": "mocha src/**/*.spec.js -r babel-register -r ./test/init --exit",
    "test:modules:watch": "run-s 'test:modules -- --watch'",
    "test": "run-s test:run test:modules",
    "test-prod": "NODE_ENV=production run-s test:run test:modules",
    "postinstall": "node src/postInstall && chmod +x ace",
    "lint": "eslint --fix src/",
    "db:sync": "./ace db-sync",
    "db:seed": "./ace db-seed",
    "db:init": "run-s -s db:sync db:seed"
  },
  "dependencies": {
    "@types/node": "^10.12.18",
    "accesscontrol": "^2.2.1",
    "aws-sdk": "^2.382.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcrypt": "^3.0.3",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~4.1.1",
    "deepmerge": "^3.0.0",
    "dotenv": "^6.2.0",
    "exceljs": "^1.6.3",
    "express": "^4.16.4",
    "express-fileupload": "^1.0.0",
    "express-zip": "^2.0.1",
    "faker": "^4.1.0",
    "handlebars": "^4.0.12",
    "helmet": "^3.15.0",
    "http-errors": "~1.7.1",
    "jsonschema": "^1.2.4",
    "module-alias": "^2.1.0",
    "moment": "^2.23.0",
    "morgan": "~1.9.0",
    "mysql2": "^1.6.4",
    "nexmo": "^2.4.0",
    "node": "^11.5.0",
    "nodemailer": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "rimraf": "^2.6.2",
    "sequelize": "^4.42.0",
    "sql-ddl-to-json-schema": "^2.2.0",
    "uuid": "^3.3.2",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "acorn": "^6.0.4",
    "babel-eslint": "^10.0.1",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "eslint": "^5.11.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^1.2.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "sequelize-cli": "^5.4.0",
    "sinon": "^7.2.2"
  },
  "husky": {
    "hooks": {}
  }
}
